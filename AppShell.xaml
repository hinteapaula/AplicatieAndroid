<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PostMAUIApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PostMAUIApp"
    Shell.FlyoutBehavior="Disabled">


    <TabBar>
        <ShellContent
            Title="FastCourier"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
        <ShellContent
            Title="About"
            ContentTemplate="{DataTemplate local:About}"
            Route="About" />
        <ShellContent
            Title="Packages"
            ContentTemplate="{DataTemplate local:EntryListPage}"
            Route="EntryListPage"
             /> 
    </TabBar>
    

</Shell>
